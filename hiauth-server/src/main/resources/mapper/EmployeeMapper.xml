<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.hiauth.server.mapper.EmployeeMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="Employee">
        <result column="id" property="id" />
        <result column="cid" property="cid" />
        <result column="user_id" property="userId" />
        <result column="no" property="no" />
        <result column="name" property="name" />
        <result column="email" property="email" />
        <result column="creator" property="creator" />
        <result column="updater" property="updater" />
        <result column="deleter" property="deleter" />
        <result column="create_time" property="createTime" />
        <result column="update_time" property="updateTime" />
        <result column="delete_time" property="deleteTime" />
        <result column="is_deleted" property="isDeleted" />
        <result column="is_corp_admin" property="isCorpAdmin" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, cid, user_id, no, name, email, creator, updater, deleter, create_time, update_time, delete_time, is_deleted, is_corp_admin
    </sql>

</mapper>
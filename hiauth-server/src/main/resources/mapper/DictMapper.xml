<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.hiauth.server.mapper.DictMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="Dict">
        <result column="id" property="id" />
        <result column="cid" property="cid" />
        <result column="sort" property="sort" />
        <result column="code" property="code" />
        <result column="p_code" property="pCode" />
        <result column="name" property="name" />
        <result column="value" property="value" />
        <result column="is_enable" property="isEnable" />
        <result column="create_time" property="createTime" />
        <result column="has_child" property="hasChild" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, cid, sort, code, p_code, name, value, is_enable, create_time
    </sql>

</mapper>